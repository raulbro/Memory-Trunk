<div class="container">
	<h1 class="h1_title">New picture</h1>
	
	<%= form_for @picture, :url => {action: "create"}, :html => { :multipart => true} do |f| %>

      <div class="row">
      	<div class="col s12 m6">
      		<%= f.label "Add new picture" %>
      		<%= f.file_field :picture_data %>
      	</div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <%= button_tag( :class => "waves-effect waves-light btn green") do %>
            Create <i class="material-icons right">done</i>
          <% end %>
        </div>
      </div>
  <% end %>
</div>